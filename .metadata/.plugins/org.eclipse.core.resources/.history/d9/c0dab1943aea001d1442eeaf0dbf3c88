package array;

import java.util.Arrays;

public class Array2 {
	public static void main(String args[])
	{
	int a[]=	method();
	System.out.println(Arrays.toString(a));
		
	}
	public static int[] method()
	{
		int a[]= {-1,-1};
		int b[]= {2,3,4,1};
		int c=5;
	int d[]=new int[3];
		for(int i=0;i<b.length;i++)
		{
			if(c==b[i])
			{
				d[i]=i;
			}
			else
				
			{
				return a;
			}
		}
		return d;
	}

}
